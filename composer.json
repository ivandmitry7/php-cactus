{
  "name": "notihnio/php-cactus",
  "bin": [
    "bin/cactus"
  ],
  "description": "Compile and protect your php code from been stolen. No risk deployments to client's servers",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Notis Mastrandrikos",
      "email": "pmastrandrikos@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Notihnio\\Cactus\\": "src/"
    }
  },
  "require": {
    "php": ">=5.6",
    "ext-zend-opcache": "*",
    "ext-fileinfo": "*"
  },
  "repositories": [
        {
            "type": "path",
            "url": "../php-cactus",
            "options": { "symlink": true }
        }
    ],

    "require": {
        "notihnio/php-cactus": "@dev"
    },

    "prefer-stable": true,
  
  "minimum-stability": "stable"
}
